{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "default": "Warning",
            "Function.Clear.User": "Information",
            "Function.Start.User": "Information",
            "Function.Collector.User": "Information",
            "Function.Processor.User": "Information",
            "Function.Processor_ProcessPartition.User": "Information"
        }
    },
    "extensions": {
        "durableTask": {
          "extendedSessionsEnabled": true,
          "extendedSessionIdleTimeoutInSeconds": 30
        }
      }
}